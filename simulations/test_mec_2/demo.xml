<config>
    <!-- Assign to the wlan interface of all eNodeBs and UEs one address of type 10.x.x.x, since dynamic-created UEs will be assigned one such address -->
    <interface hosts="" names="wlan" address="10.x.x.x" netmask="255.0.0.0"/>
    
    
    <interface hosts="meHost.virtualisationInfrastructure" names="ppp0" address="195.168.5.x" netmask="255.255.255.0"/>
    <interface hosts="meHost.gtpEndpoint" names="pppInterface" address="195.168.5.x" netmask="255.255.255.0"/>

    <!-- Assign 195.168.x.x addresses to all other interfaces -->
    <interface hosts="**" address="195.168.x.x" netmask="255.255.254.0"/>
    
     
	
	
	<!-- external destination 192.168.1.175-->
	<route hosts="ue*" destination="192.168.1.0" netmask="255.255.255.0" interface="wlan"/>
	<route hosts="eNodeB*" destination="192.168.1.0"  netmask="255.255.255.255" interface="pppMEHostIf"/>
	<route hosts="meHost.gtpEndpoint*" destination="192.168.1.0" netmask="255.255.255.0" interface="pppInterface"/>

    <!-- all D2D-capable UEs participate in the multicast group --> 
    <multicast-group hosts="car*" interfaces="wlan" address="224.0.0.10"/>
</config>
